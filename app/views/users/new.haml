/ %form{action: "/rides", method: "post"}
/   First Name
/   %br
/   %input{type:"text", name:"user[first_name]", id:"first_name"}
/   %br
/   Last Name
/   %br
/   %input{type:"text", name:"user[last_name]", id:"last_name"}
/   %br
/   %input{type:"submit", value:"submit"}

= form_with(url: "/users", method: "POST") do
  = label_tag(:first_name, "First Name")
  = text_field_tag(:first_name)
  %br
  = label_tag(:last_name, "Last Name")
  = text_field_tag(:last_name)
  %br
  = label_tag(:contact_number, "contact number")
  = text_field_tag(:contact_number)
  %br
  = label_tag(:email, "email id")
  = text_field_tag(:email)
  %br
  = label_tag(:password, "password")
  = password_field_tag(:password)
  %br
  = submit_tag("Submit")